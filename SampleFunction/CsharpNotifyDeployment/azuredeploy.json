{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "providername": {
            "type": "string",
            "defaultValue": "public"
        }
    },
    "variables": {
        "location": "eastus",
        "customrpApiversion" : "2018-09-01-preview",
        "functionendpoint" : "https://suhasalertfunc.azurewebsites.net/api/{requestPath}"
    },
    "resources": [
        {
            "apiVersion": "[variables('customrpApiversion')]",
            "type": "Microsoft.CustomProviders/resourceProviders",
            "name":"[parameters('providername')]",
            "location":"[variables('location')]",
            "properties" :{
                            "resourceTypes": [
                                {
                                    "name":"alerts",
                                    "routingType":"Proxy,Cache",
                                    "endpoint": "[variables('functionendpoint')]"
                                }
                            ]
                        }
        },
        {
            "type": "Microsoft.CustomProviders/resourceProviders/alerts",
            "name": "[concat(parameters('providername'), '/alert1')]",
            "apiVersion": "2018-09-01-preview",
            "location": "eastus",
            "properties": {
                "name": "Evan Hissey",
                "number": "5743441978"
            },
            "dependsOn": [
                "[concat('Microsoft.CustomProviders/resourceProviders/',parameters('providername'))]"
            ]
        }
    ],
    "outputs": {
    }
}